{
  "rabbit_version": "3.12.0",
  "users": [],
  "vhosts": [
    {
      "name": "hivemind"
    }
  ],
  "permissions": [
    {
      "user": "hivemind",
      "vhost": "hivemind",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "parameters": [],
  "policies": [
    {
      "vhost": "hivemind",
      "name": "dlq-policy",
      "pattern": "^hivemind\\.tasks$",
      "apply-to": "queues",
      "definition": {
        "dead-letter-exchange": "",
        "dead-letter-routing-key": "hivemind.tasks.dlq"
      },
      "priority": 0
    },
    {
      "vhost": "hivemind",
      "name": "message-ttl",
      "pattern": ".*",
      "apply-to": "queues",
      "definition": {
        "message-ttl": 86400000
      },
      "priority": 0
    }
  ],
  "queues": [
    {
      "name": "hivemind.tasks",
      "vhost": "hivemind",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-dead-letter-exchange": "",
        "x-dead-letter-routing-key": "hivemind.tasks.dlq",
        "x-max-priority": 5
      }
    },
    {
      "name": "hivemind.tasks.dlq",
      "vhost": "hivemind",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "hivemind.events",
      "vhost": "hivemind",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "exchanges": [
    {
      "name": "hivemind.events",
      "vhost": "hivemind",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "hivemind.direct",
      "vhost": "hivemind",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "hivemind.events",
      "vhost": "hivemind",
      "destination": "hivemind.events",
      "destination_type": "queue",
      "routing_key": "#",
      "arguments": {}
    },
    {
      "source": "hivemind.direct",
      "vhost": "hivemind",
      "destination": "hivemind.tasks",
      "destination_type": "queue",
      "routing_key": "tasks",
      "arguments": {}
    }
  ]
}
